                  <ref name="model.global"/>
                  <zeroOrMore>
                     <ref name="desc"/>
                  </zeroOrMore>
               </choice>
            </zeroOrMore>
            <choice>
               <oneOrMore>
                  <group>
                     <ref name="item"/>
                     <zeroOrMore>
                        <ref name="model.global"/>
                     </zeroOrMore>
                  </group>
               </oneOrMore>
               <group>
                  <oneOrMore>
                     <group>
                        <ref name="label"/>
                        <zeroOrMore>
                           <ref name="model.global"/>
                        </zeroOrMore>
                        <ref name="item"/>
                        <zeroOrMore>
                           <ref name="model.global"/>
                        </zeroOrMore>
                     </group>
                  </oneOrMore>
               </group>
            </choice>
            <zeroOrMore>
               <group>
                  <ref name="model.divBottom"/>
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
               </group>
            </zeroOrMore>
